{
    "manifestVersion": 1.0,
    "id": "RESTRequestButton",
    "publisher": "Apodemus",
    "version": "1.0.70",
    "public": true,
    "name": "Work Item REST Request Button",
    "description": "Adds a custom control with a button that sends a REST request.",
    "categories": [
        "Azure Boards"
    ],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "logo.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "scopes": [
        "vso.work"
    ],
    "files": [
        {
            "path": "static",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        }
    ]
}